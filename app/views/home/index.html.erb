<h1>Rooms</h1>
<div class='container'>
  <% Room.all.each do |room| %>
    <div>
      <%= room.name   %> room
      <%= link_to "Room", room_messages_path(room) %>
    </div>
  <% end %>

  <%= form_for Room.new do |f| %>
    <%= f.text_field :name %>
    <%= f.submit "Create room" %>
  <% end %>

</div>